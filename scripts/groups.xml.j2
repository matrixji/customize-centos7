<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE comps PUBLIC "-//CentOS//DTD Comps info//EN" "comps.dtd">
<comps>
  <group>
    <id>core</id>
    <name>Core</name>
    <description>{{ os_name }} Core Packages</description>
    <default>true</default>
    <uservisible>true</uservisible>
    <packagelist>
{% for rpm in install_rpms %}}      <packagereq type="mandatory">{{ rpm }}</packagereq>{% endfor %}
    </packagelist>
  </group>
  <environment>
    <id>all</id>
    <name>{{ os_name }} Core Packages</name>
    <display_order>5</display_order>
    <grouplist>
      <groupid>core</groupid>
    </grouplist>
    <optionlist>
    </optionlist>
  </environment>
</comps>
